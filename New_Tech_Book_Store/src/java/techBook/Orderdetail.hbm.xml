<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="techBook.Orderdetail" table="ORDERDETAIL" schema="TECHBOOK">
        <id name="orderid" type="int">
            <column name="ORDERID" />
            <generator class="increment" />
        </id>
        <property name="bookid" type="string">
            <column name="BOOKID" length="10" />
        </property>
        <property name="qty" type="java.lang.Integer">
            <column name="QTY" />
        </property>
        <property name="shippingaddress" type="string">
            <column name="SHIPPINGADDRESS" length="100" />
        </property>
    </class>
</hibernate-mapping>